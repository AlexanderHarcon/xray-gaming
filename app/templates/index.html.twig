        {% for category, products in productsMap %}
        <h2 class="ttl">{{ category }}</h2>
        <div class="content">
            <div class="contentProducts">
                {% for product in products %}
                    <div class="contentProductsItem">
                        <a href="{{ path('case_page') }}"><img src="assets/img/{{ product.imagePath }}" alt="" width="225px" height="225px"></a>
                        <span class="label discount">4 дня</span>
                        <h4>{{ product.name }}</h4>
                        <a class="btn" href="{{ path('case_page') }}">{{ product.price }} UAH</a>
                    </div>
                {% endfor %}
            </div>
            {% endfor %}
        </div>
