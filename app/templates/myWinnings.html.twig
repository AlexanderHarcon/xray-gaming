<div id="takeAway" class="modal">
    <div class="wrapper">
        <div class="modalWrap">

            <div class="modalHead">
                <h2>Забрать <img src="assets/img/coin.svg" alt="Coins"></h2>
                <span class="close">
                    <img src="assets/img/cross.svg" alt="Close modal">
                </span>
            </div>

            <div class="modalBody">

                <form class="form" action="">
                    <div class="formField">
                        <label for="lastName">
                            Фамилия
                        </label>
                        <input type="text" placeholder="Усачев" name="lastName" id="lastName">
                    </div>

                    <div class="formField">
                        <label for="name">
                            Имя
                        </label>
                        <input type="text" placeholder="Руслан" name="name" id="name">
                    </div>

                    <div class="formField">
                        <label for="middleName">
                            Отчество
                        </label>
                        <input type="text" placeholder="Ренатович" name="middleName" id="middleName">
                    </div>

                    <div class="formField">
                        <label for="phone">
                            Телефон
                        </label>
                        <input type="tel" placeholder="+7 (911) 777 56 67 " name="phone" id="phone">
                    </div>

                    <div class="formField">
                        <label for="city">
                            Город
                        </label>
                        <input type="text" placeholder="Москва" name="city" id="city">
                    </div>

                    <div class="formField">
                        <label for="street">
                            Улица
                        </label>
                        <input type="text" placeholder="Светлая" name="street" id="street">
                    </div>

                    <div class="formField">
                        <div class="formFieldCol">
                            <label for="house">
                                Дом
                            </label>
                            <input type="text" placeholder="3" name="house" id="house">
                        </div>
                        <div class="formFieldCol">
                            <label for="housing">
                                Корпус
                            </label>
                            <input type="text" placeholder="2" name="housing" id="housing">
                            <p>если есть</p>
                        </div>
                        <div class="formFieldCol">
                            <label for="apartment">
                                Квартира
                            </label>
                            <input type="text" placeholder="156" name="apartment" id="apartment">
                        </div>
                    </div>

                    <div class="formField">
                        <label for="index">
                            Индекс
                        </label>
                        <input type="text" placeholder="56887" name="index" id="index">
                    </div>

                    <div class="submit">
                        <button id="takeCoins" type="submit">Забрать</button>
                        <button class="closeBtn" type="reset">Отмена</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

